<% if current_landlord %>
  <%= link_to 'Sign out', destroy_landlord_session_path, :method => :delete %>
<% else %>
  <%= link_to 'Landlord Sign up', new_landlord_registration_path %>
  <%= link_to 'Landlord Sign in', new_landlord_session_path %>
<% end %>

<% if current_user %>
  <%= link_to 'Sign out', destroy_user_session_path, :method => :delete %>
<% else %>
  <%= link_to 'User Sign up', new_user_registration_path %>
  <%= link_to 'User Sign in', new_user_session_path %>
<% end %>
